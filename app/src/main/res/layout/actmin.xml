<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/draw"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainAct">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/tabel"
                android:layout_below="@+id/carouselView">
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:background="#FFFFFF"
                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="القائمـــــــــــة"
                        android:textColor="#FF9800"
                        android:gravity="center"
                        android:textSize="30sp"
                        android:fontFamily="@font/fontara"
                        android:layout_margin="10dp"/>
                </TableRow>
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:background="#FFC107">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="ماكولات"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:textSize="20sp"
                        android:fontFamily="@font/fontara"
                        android:layout_margin="10dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="حلويات"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:layout_margin="10dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="عصائر"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:layout_margin="10dp"/>


                </TableRow>
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/bg_round">
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:scaleType="centerCrop"
                        android:layout_margin="5dp"

                        android:src="@drawable/f"
                        android:id="@+id/image1"
                        />
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"
                        android:src="@drawable/b"
                        android:scaleType="centerCrop"
                        android:id="@+id/image2"
                        />
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:scaleType="centerCrop"
                        android:layout_margin="5dp"
                        android:src="@drawable/a"
                        android:id="@+id/image3"
                        />


                </TableRow>
         <!--Favored-->
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:background="#FFFFFF"
                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="مفضلتــــــــي"
                        android:textColor="#FF9800"
                        android:gravity="center"
                        android:textSize="30sp"
                        android:fontFamily="@font/fontara"
                        android:layout_margin="10dp"/>
                </TableRow>
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:background="#FFC107">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="ماكولات"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:textSize="20sp"
                        android:fontFamily="@font/fontara"
                        android:layout_margin="10dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="حلويات"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:layout_margin="10dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="عصائر"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:layout_margin="10dp"/>


                </TableRow>
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:background="@drawable/bg_round"
                    android:layout_marginTop="5dp">
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"
                        android:src="@drawable/x"
                        android:scaleType="centerCrop"
                        android:id="@+id/fav1"
                        />
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"
                        android:src="@drawable/d"
                        android:scaleType="centerCrop"
                        android:id="@+id/fav2"
                        />
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"
                        android:src="@drawable/z"
                        android:scaleType="centerCrop"
                        android:id="@+id/fav3"
                        />


                </TableRow>
         <!--Serch By intg-->
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:background="#FFFFFF"
                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="إبحث بالمكونات المتاحة"
                        android:textColor="#FF9800"
                        android:gravity="center"
                        android:textSize="20sp"
                        android:fontFamily="@font/fontara"
                        android:layout_margin="10dp"/>
                </TableRow>
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:background="#FFC107">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="ماكولات"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:textSize="20sp"
                        android:fontFamily="@font/fontara"
                        android:layout_margin="10dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="حلويات"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:layout_margin="10dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="عصائر"
                        android:textColor="#000000"
                        android:gravity="center"
                        android:layout_margin="10dp"/>


                </TableRow>
                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:weightSum="3"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/bg_round">
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:scaleType="centerCrop"
                        android:layout_margin="5dp"
                        android:src="@drawable/eeee"
                        android:id="@+id/s1"
                        />
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"
                        android:src="@drawable/ee"
                        android:scaleType="centerCrop"
                        android:id="@+id/s2"
                        />
                    <ImageView
                        android:layout_width="130dp"
                        android:layout_height="150dp"
                        android:layout_weight="1"
                        android:scaleType="centerCrop"
                        android:layout_margin="5dp"
                        android:src="@drawable/eee"
                        android:id="@+id/s3"
                        />


                </TableRow>
            </TableLayout>





            <com.synnapps.carouselview.CarouselView
                android:id="@+id/carouselView"
                android:layout_width="match_parent"
                android:layout_height="300dp"
                app:fillColor="#FFFFFFFF"
                app:pageColor="#00000000"
                app:radius="6dp"
                app:slideInterval="3000"
                app:strokeColor="#FF777777"
                app:strokeWidth="1dp" />


        </RelativeLayout>
    </ScrollView>





    <com.google.android.material.navigation.NavigationView
        app:headerLayout="@layout/header"
        android:id="@+id/a"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:itemTextColor="#000000"
        app:itemIconTint="#ff6600 "
        app:menu="@menu/drawermenu"
        android:layout_gravity="start"
        >



    </com.google.android.material.navigation.NavigationView>
</androidx.drawerlayout.widget.DrawerLayout>